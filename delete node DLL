class Node:
    def __init__(self, data):
        self.data = data
        self.prev = None
        self.next = None

def deleteNode(head, x):
    # Case: delete head
    if x == 1:
        new_head = head.next
        if new_head:
            new_head.prev = None
        return new_head

    curr = head
    count = 1

    # Traverse to the x-th node
    while curr and count < x:
        curr = curr.next
        count += 1

    # Update pointers
    if curr.prev:
        curr.prev.next = curr.next
    if curr.next:
        curr.next.prev = curr.prev

    return head
